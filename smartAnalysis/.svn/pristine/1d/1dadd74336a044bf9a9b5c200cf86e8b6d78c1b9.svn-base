<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="board">
  
  <select id="getBoardnum" parameterType="int" resultType="int">
  	select board_num
  		from board
  		where cship_num = #{id}
  </select>
  
  </mapper>