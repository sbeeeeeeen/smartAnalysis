<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="floating">
	<select id="getFloatingVo" parameterType="long" resultType="floatingVo">
		SELECT *
		FROM floating
		WHERE reg_code = #{reg_code}
	</select>
</mapper>