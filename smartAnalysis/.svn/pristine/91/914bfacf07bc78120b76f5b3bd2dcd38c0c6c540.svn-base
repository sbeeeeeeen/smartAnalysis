/* carousel */
.carousel {
  position: relative;
}
.carousel-inner {
  position: relative;
  width: 100%;
  overflow: hidden;
}
.carousel-inner > .item {
  position: relative;
  display: none;
  transition: .6s ease-in-out left;
}
.carousel-inner > .item > img,
.carousel-inner > .item > a > img {
  line-height: 1;
}
.carousel-inner > .active,
.carousel-inner > .next,
.carousel-inner > .prev {
  display: block;
}
.carousel-inner > .active {
  left: 0;
}
.carousel-inner > .next,
.carousel-inner > .prev {
  position: absolute;
  top: 0;
  width: 100%;
}
.carousel-inner > .next {
  left: 100%;
}
.carousel-inner > .prev {
  left: -100%;
}
.carousel-inner > .next.left,
.carousel-inner > .prev.right {
  left: 0;
}
.carousel-inner > .active.left {
  left: -100%;
}
.carousel-inner > .active.right {
  left: 100%;
}
.carousel-indicators {
  position: absolute;
  top: 20px;
  right: 30px;
  width: 100%;
  text-align: right;
  list-style: none;
}
.cbanner .carousel-indicators {
  position: absolute;
  top: 10px;
  right: 15px;	
}
.carousel-indicators li {
  display: inline-block;
  width: 10px;
  height: 10px;
	margin:0 0 0 5px;
	padding:0;
  text-indent: -999px;
  cursor: pointer;
  background-color: #b2bfc9;
  border-radius: 10px;
}
.carousel-indicators .active {
  width: 10px;
  height: 10px;
  background-color: #008ed5;
}
.carousel-caption {
  position: absolute;
  right: 30px;
  top: 15px;
  left: 15%;
  padding-top: 20px;
  padding-bottom: 20px;
	font-weight:700;
  text-align: right;  
}
