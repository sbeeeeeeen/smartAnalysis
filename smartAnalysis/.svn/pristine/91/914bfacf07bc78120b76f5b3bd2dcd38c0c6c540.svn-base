/* Modal */
.modal {
	z-index:99999;
	display: none; 
	position: fixed;
	padding-top:50px; 
	left: 0;
	top: 0;
	width: 100%; 
	height: 100%; 
	overflow: auto; 
	background-color: rgb(0,0,0); 
	background-color: rgba(0,0,0,0.9); 
}
.modal-content {
	position: relative;
	overflow:auto;
	overflow:hidden;
	background-color:rgba(0,0,0,0.1); 
	margin:auto;
	border: 1px solid #333;
	width:1000px;
	animation-name: animatetop;
	animation-duration: 0.2s;
	padding:10px 10px;
}
@keyframes animatetop {
	from {top:-300px; opacity:0}
	to {top:0; opacity:1}
}
